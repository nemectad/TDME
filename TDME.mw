# Computing the transition dipole matrix elements
grad := proc(expr)  local gr := &lt;0, 0, 0&gt;;  gr[1] := simplify(diff(expr,p_x));  gr[2] := simplify(diff(expr,p_y));  gr[3] := simplify(diff(expr,p_z));  return grend proc:
dipoleElement := proc(expr)
  local d_p;
  d_p := I*grad(expr);
  return d_p;
end proc:</Text-field>
</Input>
